                 ps.setInt(2, totCapacity);
                 ps.setInt(3, capacity);
                ps.setBoolean(4, AC);
                 ps.setInt(5, profitPerBus);
                 ps.setInt(6, totalEarnings);
      int rowsInserted = ps.executeUpdate();


