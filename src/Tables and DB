To create Bus table
  create table bus(id int PRIMARY KEY AUTO_INCREMENT,seatNo int DEFAULT 332,totCapacity int NOT NULL,capacity int,AC bool,profitPerBus int,totalEarnings int,pricePerTicket int);
  insert into bus values(?,?,?,?,?,?)
