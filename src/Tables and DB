


To create Bus table
  create table bus(id int PRIMARY KEY AUTO_INCREMENT,seatNo int DEFAULT 332,totCapacity int NOT NULL,capacity int,AC bool,profitPerBus int,totalEarnings int,pricePerTicket int);

To create Customer table
  create table booking(ticketId INT DEFAULT 23413 PRIMARY KEY AUTOINCREMENT,nameOfUser varchar(30),busNo int,seats int,totalCost int,seatNo varchar(255));

To create admin Table

   create table admin(username varchar(30),password varchar(30) not null);
